{
  "networking": {
    "allowedIpRanges": ["AzureFrontDoor.Backend"]
  },
  "forwardingGateway": {
    "requiredHeaders": {
      "X-Azure-FDID": "419698b6-af8b-479f-8aa0-2e465ddda39a"
    },
    "allowedForwardedHosts": ["jamesshopland.cloud", "www.jamesshopland.cloud"]
  },
  "routes": [
    {
      "route": "/login",
      "rewrite": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  }
}
