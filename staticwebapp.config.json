{
    "routes": [
      {
        "route": "/login",
        "rewrite": "/.auth/login/github?post_login_redirect_uri=https://www.jamesshopland.cloud/loggedin"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      },
      {
        "route": "/loggedIn",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "400": {
        "rewrite": "/index.html"
      },
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "403": {
        "rewrite": "/index.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    }
  }